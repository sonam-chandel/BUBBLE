#include<stdio.h>
int main()
{
    int i,a[100],n,j,temp;
    printf("enter size of array");
    scanf("%d",&n);
    printf("enter element");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n-1;i++)
    {
        for(j=0;j<n-1-i;j++)
        {
            if(a[j]>a[j+1])
            {
                temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;

            }
        }
    }

    for(i=0;i<n;i++){
        printf(" sorted array elememt are:%d\n",a[i]);
    }
    return 0;
}
